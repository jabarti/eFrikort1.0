<wsdl:definitions name='RegisterCardInteraction'
    xmlns:wsdl='http://schemas.xmlsoap.org/wsdl/'
    xmlns:soap='http://schemas.xmlsoap.org/wsdl/soap/'
    xmlns:xs='http://www.w3.org/2001/XMLSchema'
    xmlns:tjsr='urn:com:logica:se:healthcare:efrikort:administration:RegisterCardResponder:2'
    xmlns:itr='urn:riv:itintegration:registry:1'
    xmlns:tns='urn:com:logica:se:healthcare:efrikort:administration:RegisterCard:2:rivtabp21'
    targetNamespace='urn:com:logica:se:healthcare:efrikort:administration:RegisterCard:2:rivtabp21'>

    <wsdl:documentation xml:lang='sv'>
         Tjänsteinteraktionens namn: RegisterCard
         Beskrivning: 
           Denna tjänst används för att registrera ett frikort utan transaktioner.
           Kortet kommer per automatik få frikortsstatus preliminärt.
           När frikortet har registrerats, registreras även patientens samtycke till att använda tjänsten.
         Revisioner: 
         Tjänstedomän: efrikort:administration
         Tjänsteinteraktionstyp: Fråga-Svar
         WS-profil: RIVTABP21
         Förvaltas av: Inera AB
    </wsdl:documentation>
    
    <wsdl:types>
        <xs:schema targetNamespace='urn:com:logica:se:healthcare:efrikort:administration:RegisterCard:2:rivtabp21'>
            <xs:import schemaLocation='RegisterCardResponder_2.0.xsd' namespace='urn:com:logica:se:healthcare:efrikort:administration:RegisterCardResponder:2' />
            <xs:import schemaLocation='../../../core_components/itintegration_registry_1.0.xsd' namespace='urn:riv:itintegration:registry:1' />
        </xs:schema>
    </wsdl:types>
    
        
    <wsdl:message name='RegisterCardRequest'>
        <wsdl:part name='LogicalAddress' element='itr:LogicalAddress'>
            <wsdl:documentation xml:lang='sv'>http://tempuri.org</wsdl:documentation>
        </wsdl:part>
    
        <wsdl:part name='parameters' element='tjsr:RegisterCardRequest' />
    </wsdl:message>

    <wsdl:message name='RegisterCardResponse'>
        <wsdl:part name='parameters' element='tjsr:RegisterCardResponse' />
    </wsdl:message>

    <wsdl:portType name='RegisterCardResponderInterface'>
        <wsdl:operation name='RegisterCard'>
            <wsdl:input message='tns:RegisterCardRequest' />
            <wsdl:output message='tns:RegisterCardResponse' />
        </wsdl:operation>
    </wsdl:portType>

    <wsdl:binding name='RegisterCardResponderBinding' type='tns:RegisterCardResponderInterface'>
        <soap:binding style='document' transport='http://schemas.xmlsoap.org/soap/http' />
        <wsdl:operation name='RegisterCard'>
            <soap:operation soapAction='urn:com:logica:se:healthcare:efrikort:administration:RegisterCardResponder:2:RegisterCard' style='document' />
            <wsdl:input>
                <soap:header use='literal' message='tns:RegisterCardRequest' part='LogicalAddress' />
                <soap:body use='literal' parts='parameters' />
            </wsdl:input>
            <wsdl:output>
                <soap:body use='literal' />
            </wsdl:output>
        </wsdl:operation>
    </wsdl:binding>

    <wsdl:service name='RegisterCardResponderService'>
        <wsdl:port name='RegisterCardResponderPort' binding='tns:RegisterCardResponderBinding'>
            <soap:address location='http://tempuri.org' />
        </wsdl:port>
    </wsdl:service>
</wsdl:definitions>
